{
  "name": "stash-it",
  "version": "1.2.1",
  "description": "Cache mechanism based on plugins.",
  "author": "Jacek Smolak <jacek.smolak@gmail.com>",
  "main": "build/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/smolak/stash-it.git"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "lint": "eslint --ext '.js' --ext '.md' --cache ./src",
    "lint:fix": "eslint --ext '.js' --ext '.md' --cache --fix ./src",
    "prepare": "NODE_ENV=production babel src --presets babel-preset-es2015 --out-dir build",
    "test": "NODE_ENV=test nyc npm run test:unit && nyc check-coverage",
    "test:unit": "NODE_ENV=test mocha $(find src test -path '*.test.js') --opts test/unit/mocha.opts",
    "test:file": "NODE_ENV=test mocha --opts test/unit/mocha.opts --reporter spec -w "
  },
  "keywords": [
    "cache",
    "plugins"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.4.1",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "eslint": "^4.4.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-markdown": "^1.0.0-beta.7",
    "eslint-plugin-mocha": "^4.11.0",
    "mocha": "^3.5.3",
    "nyc": "^10.3.2",
    "ramda": "^0.24.1",
    "sinon": "^3.1.0",
    "sinon-chai": "^2.12.0",
    "stash-it-test-helpers": "^1.2.0"
  }
}
