{
  "name": "stash-it",
  "version": "3.0.0-rc.1",
  "description": "Cache mechanism based on plugins.",
  "author": "Jacek Smolak <jacek.smolak@gmail.com>",
  "main": "build/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/smolak/stash-it.git"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "lint": "eslint --ext '.js' --ext '.md' --cache ./src",
    "lint:fix": "eslint --ext '.js' --ext '.md' --cache --fix ./src",
    "prepare": "NODE_ENV=production babel src --presets babel-preset-env --out-dir build",
    "test": "NODE_ENV=test nyc npm run test:unit && nyc check-coverage",
    "test:unit": "NODE_ENV=test mocha $(find src test -path '*.test.js') --opts test/unit/mocha.opts",
    "test:file": "NODE_ENV=test mocha --opts test/unit/mocha.opts --reporter spec -w "
  },
  "keywords": [
    "cache",
    "plugins"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "chai": "^3.5.0",
    "eslint": "^4.19.1",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-markdown": "^1.0.0-beta.8",
    "eslint-plugin-mocha": "^5.0.0",
    "mocha": "^5.1.1",
    "nyc": "^11.7.1",
    "ramda": "^0.25.0",
    "sinon": "^5.0.7",
    "sinon-chai": "^3.0.0",
    "stash-it-test-helpers": "^1.5.1"
  }
}
